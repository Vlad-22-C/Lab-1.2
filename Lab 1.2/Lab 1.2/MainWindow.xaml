        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="350">
    <Grid>
        <TextBox x:Name="TextField" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0" Width="200"/>
